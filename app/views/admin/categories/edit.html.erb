<% @page_title = "edit admin category: #{@category.name}" %>
<section>
<%= link_to("back to list", admin_categories_path, :class => "back-link") %>

<div class= "category edit">
  <h2>Edit Admin Category</h2>
  <%= form_for @category  do |f| %>
    <%= render "form", {f: f} %>
    <div class="form-bottons">
      <%= submit_tag I18n.t "admin.category.updatesubmit" %>
    </div>
  <% end %>
</div>
</section>